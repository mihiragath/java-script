document.getElementById("id_name").className; //give current class name
document.getElementById("id_name").getAttribute("attribute_name"); //to get current attribute name
document
  .getElementById("id_name")
  .setAttribute("attribute_name", "set_attribute"); //to change or set current attribute name
const variable_name = document.getElementById("id_name"); //to store the data into variables
variable_name.style.css_attribute = "value"; //to set css on the attributes
variable_name.innerText; //it show current disply content
variable_name.textContent; //it show all content which are hiding by css
variable_name.innerHTML; // it gives all value with attribute and give all tag
document.querySelector("attribute_name"); //select first attribute which are selected in parameter
document.querySelectorAll("attribute_name"); //select all attribute which are selected in parameter into whole webpage
document.getElementsByClassName("class_name"); //to fetch the class by using name
document.createElement("element name"); //create an new element
document.createTextNode("Hello User"); // create an text node print value which is hello user
div.appendChild(variable_name); //create new child of main node
document.querySelector("li:nth-child(index_num)"); //select nth child to perform some task
variable_name.replacewith(method / variable_name); //replace with child or another method..
